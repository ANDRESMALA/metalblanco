<html xmlns="http://www.w3c.org/1999/xhtml"
        xmlns:h="http://java.sun.com/jsf/html"
        xmlns:f="http://java.sun.com/jsf/core"
       
 <h:head>
        <title>log</title>
    </h:head> 
<h:body>  
      <p>Login to access secure pages:</p>  
      <h:messages />  
      <h:form id="loginForm">  
           <h:panelGrid columns="2">  
                <h:outputLabel for="username" value="Username:" />  
                <h:inputText id="username" value="#{jSFMLogin.username}" />  
                                  
                <h:outputLabel for="password" value="Password:" />  
                <h:inputSecret id="password" value="#{jSFMLogin.password}" />  
                                  
                <h:commandButton id="loginButton" value="Login" action="#{jSFMLogin.validar()}" />  
           </h:panelGrid>  
      </h:form>  
 </h:body>  
    
    
</html>